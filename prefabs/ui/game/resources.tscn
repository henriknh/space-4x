script/source = "extends VBoxContainer

onready var node_asteroid_rocks = $Metal/Value
onready var node_power = $Power/Value

func update_ui():
	pass
	var planets = get_tree().get_nodes_in_group('Planet')

	var asteroid_rocks = 0
	var power = 0
	
	for entity in planets:
		if entity.planet_system == GameState.get_planet_system():
			metal += entity.metal
			power += entity.power
		
	node_metal.text = Formatter.round_to_dec(metal, 2) as String
	node_power.text = Formatter.round_to_dec(power, 2) as String
	

	
func _on_back_to_galaxy():
	GameState.set_planet_system(-1)
